echo "# my_repo" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin git@github.com:YeganyanEdgar/my_repo.git
git push -u origin main
$ cd ~/.ssh
$ ls
  id_rsa    id_rsa.pub
$ ssh-keygen
$ cat ~/.ssh/id_rsa.pub
ssh-rsa AAAAB3NzaC1yc2EAAAABIwAAAQEAklOUpkD...
